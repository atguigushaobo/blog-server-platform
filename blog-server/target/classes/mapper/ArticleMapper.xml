<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.blog.mapper.ArticleMapper">

    <update id="update">
        update article
        set title = #{title} ,content = #{content},image=#{image}
        where article_id = #{id}
    </update>

</mapper>